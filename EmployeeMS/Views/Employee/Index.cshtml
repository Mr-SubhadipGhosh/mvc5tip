@model IEnumerable<EmployeeModel.Models.Employee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div>@ViewBag.empBLLCheck</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.deptOfEmp.deptName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.empName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.empDoB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.empCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.version)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.deptOfEmp.deptName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.empName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.empDoB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.empCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.version)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.empId }) |
            @Html.ActionLink("Details", "Details", new { id=item.empId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.empId })
        </td>
    </tr>
}

</table>
